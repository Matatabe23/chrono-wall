{
	"$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
	"productName": "Chrono Wall",
	"version": "0.1.8",
	"identifier": "ru.qugor.chronowall",
	"build": {
		"frontendDist": "../.output/public",
		"devUrl": "http://localhost:3000",
		"beforeDevCommand": "npm run dev",
		"beforeBuildCommand": "npm run generate"
	},
	"app": {
		"windows": [
			{
				"title": "Chrono Wall",
				"width": 1000,
				"height": 600,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
